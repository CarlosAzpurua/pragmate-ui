{
	"name": "pragmate-ui",
	"version": "0.0.36",
	"description": "Pragmate UI is a collection of reusable components",
	"author": "",
	"license": "MIT",
	"template": "template/template.json",
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"params": {
		"application": {
			"token": "abcdefg123"
		}
	},
	"modules": {
		"path": "modules"
	},
	"deployment": {
		"distributions": [
			{
				"name": "web",
				"environment": "development",
				"platform": "web",
				"ports": {
					"bundles": "1001"
				}
			}
		]
	},
	"static": {
		"includes": [
			"assets"
		]
	},
	"dependencies": {
		"@beyond-js/react-18-widgets": "~0.0.5",
		"@beyond-js/reactive": "^0.0.3",
		"@beyond-js/widgets": "^0.1.3",
		"perfect-scrollbar": "^1.5.5",
		"prism-react-renderer": "^1.3.5",
		"socket.io-client": "^4.5.3",
		"prismjs": "^1.27.0",
		"swiper": "^8.4.5"
	},
	"devDependencies": {
		"@beyond-js/backend": "^0.1.1",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9"
	}
}