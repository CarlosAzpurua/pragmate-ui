{
    "name": "ui",
    "version": "0.0.33",
    "description": "Beyond UI is a collection of reusable components",
    "author": "",
    "license": "MIT",
    "scope": "bgroup",
    "layout": "main-layout",
    "template": "template/template.json",
    "languages": {
        "default": "en",
        "supported": ["en", "es"]
    },
    "params": {
        "application": {
            "token": "abcdefg123"
        }
    },
    "modules": {
        "path": "modules"
    },
    "deployment": {
        "distributions": [
            {
                "name": "web",
                "environment": "development",
                "ssr": "",
                "platform": "web",
                "ports": {
                    "bundles": "1001"
                }
            }
        ]
    },
    "static": {
        "includes": ["assets"]
    },
    "dependencies": {
        "@beyond-js/react-widgets": "^18.21.1",
        "@beyond-js/widgets": "^0.1.3",
        "perfect-scrollbar": "^1.5.5",
        "prism-react-renderer": "^1.3.5",
        "socket.io-client": "^4.5.3",
        "swiper": "^8.4.5"
    },
    "devDependencies": {
        "@beyond-js/backend": "^0.1.1",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9"
    }
}
